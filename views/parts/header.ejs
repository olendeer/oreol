<header>
    <h1>Производственно-кровельная компания</h1>
    <div class="search">
        <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
            <circle cx="7" cy="7" r="6" stroke="#C3CEDB" stroke-width="2"/>
            <path d="M15 15L12 12" stroke="#C3CEDB" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
        </svg>
        <span>Поиск</span>
    </div>
    <div class="langs">
        <span class="lang-selected submenu-btn"><span>ru</span>
            <div class="langs-list menu-list">
                <div class="menu-list-item new-submenu-item">RU</div>
                <div class="menu-list-item new-submenu-item">UA</div>
            </div>
            <img src="img/arrow-dark.png" alt="">
        </span>
    </div>
    <div class="phones">
        <span class="phone"><%=charts.phone1%></span>
        <span class="phone"><%=charts.phone2%></span>
        <span class="phone"><%=charts.phone3%></span>
    </div>
    <span class="chart"><%=charts.chart%></span>
</header>
<nav>
    <div class="nav-wrap">
        <div class="mobile-btn-menu">
            <img src="img/burger.png" alt="">
        </div>
        <a href="/" class="logo"><img src="img/logo.svg" alt=""></a>
        <div class="mobile-btn-phone">
            <img src="img/phone.png" alt="">
        </div>
        <ul class="navigation">
            <div class="mobile-search">
                <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <circle cx="7" cy="7" r="6" stroke="#C3CEDB" stroke-width="2"/>
                    <path d="M15 15L12 12" stroke="#C3CEDB" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                </svg>
                <span>Поиск</span>
            </div>
            <div class="mobile-langs">
                <div class="menu-list-item menu-list-item-active">RU</div>
                <div class="menu-list-item">UA</div>
            </div>
            <div class="mobile-nav-close">
                <span class="close1"></span>
                <span class="close2"></span>
            </div>
            <%for(let i = 0; i < links.length; i++){%>
                <%if(links[i].sublink == 'Нет' || links[i].name[0] == 'Каталог'){%>
                    <li><a href="<%=links[i].url%>"><%=links[i].name[0]%></a></li>
                <%}else if(links[i].sublink == 'Да'){%>
                    <li><span class="submenu-btn"><a><%=links[i].name[0]%></a>
                        <div class="menu-list">
                            <%for(let j = 0; j < links.length; j++){%>
                                <%if(links[j].sublink == links[i].name[0]){%>
                                    <a href="<%=links[j].url%>" class="menu-list-item"><%=links[j].name[0]%></a>
                                <%}%>
                            <%}%>
                        </div>
                        <img src="img/arrow.svg" alt="">
                    </span></li>
                <%}%>
            <%}%>
            <li><span class="call">Перезвоните мне</span></li>
        </ul>
        <div class="mobile-bg-menu"></div>
    </div>
</nav>
<div class="download-btns">
    <a href="">Скачать каталог</a>
    <a href="">Скачать прайс</a>
</div>